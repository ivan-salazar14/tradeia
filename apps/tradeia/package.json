{
  "name": "tradeia",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit --skipLibCheck",
    "type-check:src": "tsc --noEmit --skipLibCheck --project tsconfig.src.json",
    "type-check:strict": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --watchAll=false",
    "test:unit": "jest --testPathPattern=src/lib",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:component": "cypress run --component",
    "test:queue": "node test-queue-simple.js",
    "test:compliance": "node validate-api-compliance.js",
    "test:all": "npm run lint && npm run type-check && npm run test:coverage && npm run test:queue",
    "validate:api": "npm run test:compliance",
    "vercel:build": "./vercel-build.sh",
    "vercel:validate": "node vercel-post-deploy.js",
    "deploy:check": "npm run build && npm run validate:api",
    "docker:build": "docker build -t tradeia .",
    "docker:run": "docker run -p 3000:3000 tradeia",
    "docker:compose": "docker-compose up --build",
    "docker:compose:down": "docker-compose down",
    "prepare": "husky install || true"
  },
  "dependencies": {
    "@headlessui/react": "^2.0.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.56.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/ioredis": "^5.0.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.4.2",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.1",
    "firebase": "^10.8.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.0",
    "joi": "^18.0.1",
    "js-cookie": "^3.0.5",
    "next": "^15.5.0",
    "next-swagger-doc": "^0.4.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "swagger-ui-react": "^5.29.0",
    "tailwind-merge": "^2.5.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^9.0.0",
    "@next/eslint-plugin-next": "15.5.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.12",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.13.10",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.1",
    "@types/swagger-jsdoc": "^6.0.4",
    "autoprefixer": "^10.4.21",
    "cypress": "^13.10.0",
    "eslint": "^9.5.0",
    "eslint-config-next": "15.5.0",
    "husky": "^9.1.6",
    "jest": "^30.1.2",
    "jest-environment-jsdom": "^30.1.2",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.6.3"
  }
}
