{
  "version": 2,
  "builds": [
    {
      "src": "apps/landing/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "apps/tradeia/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/apps/landing"
    },
    {
      "src": "/landing(?:/(.*))?",
      "dest": "/apps/landing/$1"
    },
    {
      "src": "/app(?:/(.*))?",
      "dest": "/apps/tradeia/app/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/apps/landing/$1"
    }
  ]
}
